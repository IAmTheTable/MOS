void pic_initialize(void)
{


 uint8_t master_mask = 0xFC;
 uint8_t slave_mask = 0xFF;


 port_byte_out(0x20, 0x11);
 io_wait();
 port_byte_out(0xA0, 0x11);
 io_wait();

 port_byte_out(0x21, 0x20);
 io_wait();
 port_byte_out(0xA1, 0x28);
 io_wait();

 port_byte_out(0x21, 0x4);
 io_wait();
 port_byte_out(0xA1, 0x2);
 io_wait();

 port_byte_out(0x21, 0x01);
 io_wait();
 port_byte_out(0xA1, 0x01);
 io_wait();

 port_byte_out(0x21, master_mask);
 port_byte_out(0xA1, slave_mask);
}

uint8_t pic_read_irr(void)
{

 port_byte_out(0x20, 0x0A);

 return port_byte_in(0x20);
}

uint8_t pic_read_isr(void)
{

 port_byte_out(0x20, 0x0B);

 return port_byte_in(0x20);
}

void io_wait(void)
{


 int i;
 for (i = 0; i < 32; ++i)
  __asm__("nop");
}
