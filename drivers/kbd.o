int kbd_scan_code = 0;


void kbd_initialize(void)
{


 install_ir_handler(0x21, 0x8E, 0x8, (IRQ_HNDLR)kbd_irq);
}

void kbd_irq(void)
{


 __asm__("pusha");

 if (port_byte_in(0x64) & 0x01)
 {
  kbd_scan_code = port_byte_in(0x60);
  kbd_dispatch(kbd_scan_code);
 }

 end_of_interrupt();
 __asm__("popa");
 __asm__("leave");
 __asm__("iret");
}
